{
  "name": "lena2332/serializer2",
  "description": "This is a serializer tool for object used Json, Xml, Yaml",
  "keywords": ["serializer", "object", "xml", "json", "yaml"],
  "type": "",
  "license": "",
  "authors": [
    {
      "name": "Elena Kupriec",
      "email": "box32.lena@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "symfony/yaml": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "friendsofphp/php-cs-fixer": "^2.14"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes"
  }
}
